{
  "name": "@puge/http",
  "version": "1.0.0",
  "description": "快速启动一个Http服务",
  "scripts": {
    "precommit": "lint-staged",
    "test": "xo"
  },
  "files": [
    "bin",
    "lib",
    "assets",
    "views"
  ],
  "main": "./lib/api.js",
  "bin": {
    "http": "./bin/serve.js"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "xo": {
