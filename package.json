    "prettier": "1.15.3",
    "xo": "0.20.3"
  },
  "dependencies": {
    "args": "3.0.8",
    "basic-auth": "2.0.1",
    "bluebird": "3.5.3",
    "boxen": "1.3.0",
    "chalk": "2.4.1",
    "clipboardy": "1.2.3",
    "dargs": "5.1.0",
    "detect-port": "1.3.0",
    "filesize": "3.6.1",
    "fs-extra": "5.0.0",
    "handlebars": "4.0.12",
    "ip": "1.1.5",
    "micro": "9.3.3",
    "micro-compress": "1.0.0",
    "mime-types": "2.1.21",
    "node-version": "1.2.0",
    "openssl-self-signed-certificate": "1.1.6",
    "opn": "5.4.0",
    "path-type": "3.0.0",
    "send": "0.16.2",
    "update-notifier": "2.5.0"
  }
